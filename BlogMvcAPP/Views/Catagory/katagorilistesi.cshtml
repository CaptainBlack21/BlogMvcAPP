@model IEnumerable<BlogMvcAPP.Models.Catagory>

<div class="well">
    <h4>Blog Katagorileri</h4>
    <div class="row">

        @foreach (var item in Model)
        {
            
                <div class="col-lg-6">
                    <ul class="list-unstyled">
                        <li>
                            <a href="/Blog/List/@item.Id">@item.KatagoriAdi</a>
                        </li>
                    </ul>
                </div>
           
        }

    </div>
</div>
